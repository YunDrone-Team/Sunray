bool wp_land
# 最后是否返回起飞点并降落
bool wp_return
# 任务是否包含起飞过程
bool wp_takeoff
# 是否固定偏航角
bool wp_fix_yaw
# 航点间移动速度
float32 move_vel
# 飞行高度
float32 z_height
# 偏航角
float32 yaw_angle
# 航点 xyz 或 经纬高
float64[3] wp_point_1
