<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="wheeltec_with_mid360">

    <include>
      <uri>model://wheeltec</uri>
    </include>

   <include>
      <uri>model://livox_mid360</uri>
      <pose>0 0 0.25 0 0 0</pose>
    </include>
    <joint name="mid360_joint" type="fixed">
      <child>livox_mid360::link</child>
      <parent>wheeltec::base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>

  </model>
</sdf>
